{
	"name": "plebeian.market",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "src/index.tsx",
	"module": "src/index.tsx",
	"scripts": {
		"generate-routes": "tsr generate",
		"watch-routes": "tsr watch",
		"dev": "bun --hot src/index.tsx --host 0.0.0.0",
		"dev:seed": "bun run startup && bun run seed && bun --hot src/index.tsx --host 0.0.0.0",
		"start": "NODE_ENV=production bun run generate-routes && bun src/index.tsx",
		"build": "bun run generate-routes && bun run build.ts",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"seed": "bun run scripts/seed.ts",
		"startup": "bun run scripts/startup.ts",
		"build-icons": "bun run scripts/build-icons.ts",
		"test:e2e": "playwright test",
		"test:e2e:headed": "playwright test --headed",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:chrome": "playwright test --project=chromium",
		"test:e2e:manual": "playwright test --config=playwright-manual.config.ts --headed --project=chromium",
		"test:e2e:manual:debug": "playwright test --config=playwright-manual.config.ts --debug --project=chromium"
	},
